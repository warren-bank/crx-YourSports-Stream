{
  "name": "YourSports Stream",
  "version": "0.2.0",
  "description": "Removes clutter and reduces CPU load.",
  "content_scripts": [{
    "matches": ["*://*.yoursports.stream/*"],
    "all_frames": true,
    "run_at": "document_start",
    "js": ["content.js"]
  }],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "55": "img/icon55.png"
  },
  "manifest_version": 2
}
